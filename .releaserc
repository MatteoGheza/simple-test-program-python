{
    "repositoryUrl": "https://github.com/MatteoGheza/simple-test-program-python.git",
    "branches": ["master"],
    "plugins": [
        ["@semantic-release/commit-analyzer", {
            "preset": "angular",
            "parserOpts": {
                "noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES", "BREAKING"]
            }
        }],
        ["@semantic-release/release-notes-generator", {
            "preset": "angular",
            "parserOpts": {
              "noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES", "BREAKING"]
            },
            "writerOpts": {
                "commitsSort": ["subject", "scope"]
            }
        }],
        ["@semantic-release/github", {
            "successComment": "This ${issue.pull_request ? 'pull request' : 'issue'} is included in version ${nextRelease.version}",
            "failComment": "This release from branch ${branch.name} had failed due to the following errors:\n- ${errors.map(err => err.message).join('\\n- ')}"
        }],
        "@semantic-release/changelog",
        ["@semantic-release/exec", {
          "prepareCmd": "echo IyBET04nVCB1cGRhdGUgd2l0aCBtYW51YWxseSwgYWx3YXlzIHVzZSByZWxlYXNlIHRyaWdnZXIgZnJvbSBHaXRodWIgQWN0aW9ucy4gU2VlIFJFTEVBU0VTLm1kCiMgTk9OIG1vZGlmaWNhcmUgcXVlc3RvIGZpbGUgbWFudWFsbWVudGUsIHVzZSBzZW1wcmUgaWwgInJlbGVhc2UgdHJpZ2dlciIgZGEgR2l0aHViIEFjdGlvbnMuIEd1YXJkYSBSRUxFQVNFUy5tZAo= | base64 -d > simpletestprogram/version.py && echo __version__ = '${nextRelease.version}' >> simpletestprogram/version.py"
        }],
        ["@semantic-release/git", {
            "assets": ["CHANGELOG.md", "simpletestprogram/version.py"],
            "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }]
    ]
}